{
  "name": "algolia-cli",
  "version": "1.0.0",
  "description": "A CLI tool for common Algolia operations",
  "main": "dist/index.js",
  "bin": {
    "algolia-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linter configured. Consider adding ESLint or Biome.'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "algolia",
    "cli",
    "search"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "algoliasearch": "^5.17.0",
    "commander": "^12.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.9.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
